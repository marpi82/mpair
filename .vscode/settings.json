{
    "C_Cpp.default.compilerPath": "${env:IDF_PATH}/tools/xtensa-esp-elf/esp-13.2.0_20240530/xtensa-esp-elf/bin/xtensa-esp32s3-elf-gcc",
    "C_Cpp.default.compileCommands": "${workspaceFolder}/software/firmware-s3/build/compile_commands.json",
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/software/**",
        "${env:IDF_PATH}/components/**"
    ],
    "C_Cpp.default.browse.path": [
        "${workspaceFolder}/software",
        "${env:IDF_PATH}/components"
    ],
    "C_Cpp.default.defines": [
        "ESP_PLATFORM",
        "IDF_VER=\"v5.5.2\""
    ],
    "files.associations": {
        "*.h": "c",
        "*.c": "c",
        "sdkconfig": "properties",
        "sdkconfig.defaults": "properties"
    },
    "files.exclude": {
        "**/build": true,
        "**/.venv": true,
        "**/__pycache__": true
    },
    "search.exclude": {
        "**/build": true,
        "**/managed_components": true
    },
    "terminal.integrated.env.linux": {
        "IDF_PATH": "${env:HOME}/esp/esp-idf"
    },
    "cortex-debug.armToolchainPath": "${env:IDF_PATH}/tools/riscv32-esp-elf/esp-13.2.0_20240530/riscv32-esp-elf/bin",
    "idf.adapterTargetName": "esp32s3",
    "idf.espIdfPath": "${env:HOME}/esp/esp-idf",
    "idf.openOcdConfigs": [
        "board/esp32s3-builtin.cfg"
    ],
    "idf.port": "/dev/ttyACM0",
    "idf.baudRate": "115200",
    "python.defaultInterpreterPath": "${env:HOME}/esp/esp-idf/python_env/idf5.5_py3.10_env/bin/python",
    "editor.formatOnSave": true,
    "editor.rulers": [
        100,
        120
    ],
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "[c]": {
        "editor.defaultFormatter": "ms-vscode.cpptools",
        "editor.tabSize": 4
    },
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.tabSize": 4
    },
    "cmake.sourceDirectory": "/home/marpi82/Dokumenty/GitHub/mpair/software/firmware-c6"
}
